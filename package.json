{
  "name": "Grocerrific-Pluralsight",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "babel-node server/index.js",
    "start": "node dist/index.js",
    "test:client": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:server": "",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "build:client": "babel-node tools/buildHtml.js",
    "build:server": "babel ./server --out-dir dist/ --copy-files",
    "prebuild": "npm-run-all clean-dist build:client build:server",
    "build": "babel-node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kenec/Grocerrific-Pluralsight.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kenec/Grocerrific-Pluralsight/issues"
  },
  "homepage": "https://github.com/Kenec/Grocerrific-Pluralsight#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "mongoose": "^5.3.11",
    "path": "^0.12.7",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "sweetalert": "^2.1.2",
    "toastr": "^2.1.4",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "fs": "0.0.1-security",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.1",
    "sinon": "^7.1.1",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/client/__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/client/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/__mocks__/styleMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/components/**",
      "client/actions/*",
      "client/reducers/*"
    ]
  }
}
